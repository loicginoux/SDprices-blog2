<template id="product-results" is:raw type="x-tmpl-mustache">
  {{#products}}
    <tr>
      <td x-data='' x-show="$store.columnsDisplay.brand">{{brand}}</td>
      <td x-data='' x-show="$store.columnsDisplay.capacity">{{capacity}} GB</td>
      <td x-data='' x-show="$store.columnsDisplay.price">{{price}}€</td>
      <td x-data='' x-show="$store.columnsDisplay.price_per_gb">{{price_per_gb}}€</td>
      <td x-data='' x-show="$store.columnsDisplay.format">{{format}}</td>
      <td x-data='' x-show="$store.columnsDisplay.sd_type">{{sd_type}}</td>
      <td x-data='' x-show="$store.columnsDisplay.speed_class">{{speed_class}}</td>
      <td x-data='' x-show="$store.columnsDisplay.speed_rw">{{speed_rw}}</td>
      <td x-data='' x-show="$store.columnsDisplay.condition">{{condition}}</td>
      <td x-data='' x-show="$store.columnsDisplay.eta">{{eta}}</td>
      <td x-data='' x-show="$store.columnsDisplay.rating">
        <span class="ratings">
          <div class="ratings__wrapper">
            <span class="ratings__stars-active" style="width:{{rating}}%">
              <i class="fa fa-star" aria-hidden='true'></i>
              <i class="fa fa-star" aria-hidden='true'></i>
              <i class="fa fa-star" aria-hidden='true'></i>
              <i class="fa fa-star" aria-hidden='true'></i>
              <i class="fa fa-star" aria-hidden='true'></i>
            </span>
            <span class="ratings__stars-inactive">
              <i class="fa fa-star-o" aria-hidden='true'></i>
              <i class="fa fa-star-o" aria-hidden='true'></i>
              <i class="fa fa-star-o" aria-hidden='true'></i>
              <i class="fa fa-star-o" aria-hidden='true'></i>
              <i class="fa fa-star-o" aria-hidden='true'></i>
            </span>
          </div>
        </span>
      </td>
      <td >
        <a href="{{link}}" target="_blank">{{link}}</a>
      </td>
    </tr>
  {{/products}}
</template>